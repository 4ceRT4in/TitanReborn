{
  "schemaVersion": 1,
  "id": "titanfabric",
  "version": "${version}",
  "name": "Titan Fabric",
  "description": "Fabric Port of the Titan mod",
  "authors": [
    "4ceRT4in - Project Lead",
    "Fab - Forge dev",
    "ShiroJR - Fabric dev",
    "Globox1997 - Fabric Dev",
    "FrxzenLuke - Fabric Dev",
    "4ceRT4in - art"
  ],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/4ceRT4in/TitanReborn"
  },
  "license": "All Rights Reserved",
  "icon": "assets/titanfabric/icon.png",
  "environment": "*",
