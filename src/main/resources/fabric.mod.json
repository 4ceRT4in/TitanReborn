    "fabricloader": ">=0.17.2",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*",
    "cardinal-components-base": "^6.1.2",
    "cardinal-components-entity": "^6.1.2",
    "cardinal-components-scoreboard": "^6.1.2"
